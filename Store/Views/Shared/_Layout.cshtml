<body>

    <div class="colorlib-loader"></div>

    <div id="page">
        <nav class="colorlib-nav" role="navigation">
            <div class="top">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-4">
                            <p class="site">www.salajocuri.com</p>
                        </div>
                        <div class="col-xs-8 text-right">
                            <p class="num">Call: +01-123-456</p>
                            <ul class="colorlib-social">
                                <li><a href="#"><i class="icon-twitter"></i></a></li>
                                <li><a href="#"><i class="icon-facebook"></i></a></li>
                                <li><a href="#"><i class="icon-linkedin"></i></a></li>
                                <li><a href="#"><i class="icon-dribbble"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="navbar-collapse collapse" >
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                    <li><a asp-area="" asp-controller="Categories" asp-action="Index">Category</a></li>

                    <li><a asp-area="" asp-controller="Products" asp-action="Searching">
                            <img src="~/images/SearchLogo.png" alt="" height="22" width="42">
                        </a></li>
                    <li><a asp-area="" asp-controller="Favorits" asp-action="Index">Favorites</a></li>
                    <li>
                        <a asp-area="" asp-controller="ShoppingCarts" action="Index">
                            <img src="~/images/cart23.jpg" alt="" height="22" width="42">
                        </a>
                    </li>

                </ul>
                @await Html.PartialAsync("_LoginPartial")
            </div>

        </nav>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2020 - SalaJocuriLicenta</p>
        </footer>
    </div>

